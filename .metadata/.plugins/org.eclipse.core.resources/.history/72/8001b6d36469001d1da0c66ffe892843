<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="boardMapper">

	<select id="selectListCount" resultType="_int">
		SELECT COUNT(*)
		FROM BOARD
		WHERE STATUS = 'Y'
	</select>
	
	<select id="selectList">
		SELECT BOARD_NO
			 , BOARD_TITLE
			 , BOARD_WRITER
			 , COUNT
			 , CREATE_DATE
			 , ORIGIN_NAME
			
	</select>

</mapper>
